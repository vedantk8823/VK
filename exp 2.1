class Add {
public int add(int a, int b) {
  return a + b;  }
 public double add(double a, double b, double c) {
  return a + b + c;
    }
 public static void main(String args[]) {
 Add obj = new Add();
  System.out.println("sum of two integers: " + obj.add(25, 76));
     System.out.println("sum of three doubles: " + obj.add(20.22, 24.33, 14.7));
    }
}
